            <string>/// Shuffle the deck
var deck_size = ds_stack_size(global.deck);

if deck_size &gt; 0 {    
    shuffled = ds_list_create();
    for (i = 0; i &lt; deck_size; i++) {
        ds_list_add(shuffled, ds_stack_pop(global.deck));
